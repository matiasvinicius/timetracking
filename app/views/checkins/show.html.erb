<p id="notice"><%= notice %></p>

<p>
  <strong>Checkin comment:</strong>
  <%= @checkin.typeCheckin %>
</p>
<p>
  <strong>Latitude:</strong>
  <%= @checkin.latitude %>
</p>

<p>
  <strong>Longitude:</strong>
  <%= @checkin.longitude %>
</p>

<p>
  <strong>Created at:</strong>
  <%= @checkin.created_at %>
</p>

<p>
  <button class="btn btn-dark" onclick = "showMap(<%= @checkin.latitude %> , <%= @checkin.longitude %>)"> Show map</button>
</p>

<div id="mapholder"></div>
<script src="http://maps.google.com/maps/api/js?sensor=false"></script>

<br>

<%= link_to 'Back', checkins_path , class: "btn btn-default" %>
